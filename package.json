{
  "name": "nexium_shahzeb_faisal_grand_project",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered resume tailoring and job application tracking platform",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "set NODE_OPTIONS=--inspect && next dev",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate && supabase gen types typescript --project-id emdmbyvtzzqzuiftncte --schema public > src/types/supabase.ts",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "security:check": "npm audit --production --audit-level=critical",
    "docker:build": "docker build -t resume-tailor .",
    "docker:run": "docker run -p 3000:3000 resume-tailor"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "^5.9.1",
    "@radix-ui/themes": "^2.0.0",
    "@supabase/ssr": "^0.6.0",
    "@supabase/supabase-js": "^2.39.8",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.13.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^11.2.5",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mongodb": "^6.3.0",
    "next": "^15.4.5",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "sharp": "^0.33.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@sentry/nextjs": "^7.107.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.9.1",
    "supabase": "^2.33.5",
    "tailwindcss": "^3.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "volta": {
    "node": "18.18.2",
    "pnpm": "8.15.3"
  }
}